{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "route1": {
        "ClusterId": "summarization",
        "Match": {
          "Path": "/summarization/{**remainder}"
        }
      },
      "route2": {
        "ClusterId": "translation",
        "Match": {
          "Path": "/translation/{**remainder}"
        }
      }
    },
    "Clusters": {
      "summarization": {
        "Destinations": {
          "destination1": {
            "Address": "http://summarization.genaiplatform:8080/"
          }
        }
      },
      "translation": {
        "Destinations": {
          "destination1": {
            "Address": "http://translation.genaiplatform:8080/"
          }
        }
      }
    }
  }
}
